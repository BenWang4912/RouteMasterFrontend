@model IEnumerable<RouteMasterFrontend.EFModels.RoomProduct>
@section Styles{
	<link href="~/hotel-datepicker-main/dist/css/hotel-datepicker.css" rel="stylesheet" /><!-- Optional -->

	<style>

	</style>
}
<input id="input-id" type="text" class="form-control"/>

@section Scripts{

	<script src="~/fecha-4.2.1/fecha-4.2.1/lib/fecha.js"></script>
	<script src="~/hotel-datepicker-main/dist/js/hotel-datepicker.min.js"></script>
	<script>
		var hdpkr = new HotelDatepicker(document.getElementById("input-id"),{
			clearButton: true,
			topbarPosition: 'bottom',
			selectForward: true,
			disabledDates: [
				'2023-08-08',
				'2023-08-04',
				'2023-08-07',
				'2023-08-09',
				'2023-08-12',
				'2023-08-22'
			],
			enableCheckout: true
		});
	</script>
}

